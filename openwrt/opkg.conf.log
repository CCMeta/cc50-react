# opkg.conf  
dest root /
dest ram /tmp
lists_dir ext /data/opkg-lists
option overlay_root /overlay
option check_signature

arch all 100
arch aarch64_cortex-a55_neon-vfpv4 200
arch aarch64_cortex-a53_neon-vfpv4 300
arch aarch64_cortex-a55 400
arch aarch64_cortex-a53 500


